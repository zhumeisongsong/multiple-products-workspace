{
  "name": "time-travels-workspace",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "commit": "git-cz",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "git-cz": "^4.9.0",
    "prettier": "^3.2.5",
    "turbo": "^2.0.6",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "20.17.0",
    "pnpm": "8.15.6"
  }
}
